<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="Escalador" location="/home"/>

<shortcut
     name="Escalador"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine=" -jar $INSTALL_PATH/bin/Digrupo42013.jar"
     initialState="normal"
     iconfile="$INSTALL_PATH/icono.ico"
     iconIndex="0"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="Escalador"/> 


<shortcut
     name="Desinstalar escalador"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine=" -jar $INSTALL_PATH/Uninstaller/uninstaller.jar"
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0"
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="Escalador uninstaller"/> 


</shortcuts>
